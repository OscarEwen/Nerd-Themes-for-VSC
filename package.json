{
  "name": "arduino-themes-vsc",
  "displayName": "Arduino Theme Pack for VSC",
  "description": "The dark and light themes from the Arduion IDE imported to Visual Studio Code for your enjoyment",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/OscarEwen/nerd-themes-for-vsc"
  },
  "publisher": "OscarEwenStudent",
  "contributes": {
    "themes": [
      {
        "label": "Arduino",
        "uiTheme": "vs",
        "path": "./themes/default.color-theme.json"
      },
      {
        "label": "Arduino Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark.color-theme.json"
      }
    ]
  }
}
