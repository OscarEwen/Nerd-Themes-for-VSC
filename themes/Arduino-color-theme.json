	"tokenColors": [
		{
			"name": "",
			"settings": 
			{
			  "foreground": "#dae3e3"
			}
		},

		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"comment.block.preprocessor",
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#95a5a6cc"
			}
		},

		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Keywords",
			"scope": 
			[
			  "storage",
			  "support",
			  "keyword",
			  "string.quoted.single.c"
			],
	  
			"settings": 
			{
			  "foreground": "#00979D"
			}
		},

		{
			"name": "meta keywords",
			"scope": 
			[
			  "keyword.control",
			  "meta.preprocessor.c"
			],

			"settings": 
			{
			  "foreground": "#728E00"
			}
		},


		
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#dae3e3"
			}
		},

		{
			"name": "constants",
			"scope": 
			[
				"string.quoted.double",
				"string.quoted.other.lt-gt",
				"constant",
				"constant.language",
						"support.constant",
						"variable.language"
			],
			"settings": 
			{
				"foreground": "#005C5F"
			}
		},

		{
			"name": "Variables",
			"scope": 
			[
				"variable",
				"variable.name",
				"support.variable"
